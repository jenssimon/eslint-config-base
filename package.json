{
  "name": "@jenssimon/eslint-config-base",
  "version": "0.0.0-development",
  "description": "A shareable ESLint configuration",
  "author": "Jens Simon <https://github.com/jenssimon>",
  "type": "module",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jenssimon/eslint-config-base"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "exports": {
    ".": {
      "import": "./index.js"
    }
  },
  "scripts": {
    "lint": "eslint",
    "commit": "cz",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.35.0",
    "@stylistic/eslint-plugin": "^5.5.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.35.0",
    "eslint-config-airbnb-extended": "^2.3.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-esm": "^3.0.1",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^62.0.0",
    "husky": "^9.0.0",
    "lint-staged": "^16.0.0",
    "pinst": "^3.0.0",
    "semantic-release": "^25.0.0"
  },
  "peerDependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": ">= 4",
    "eslint": ">= 9",
    "eslint-config-airbnb-extended": ">= 2",
    "eslint-plugin-import-esm": ">= 3",
    "eslint-plugin-promise": ">= 7",
    "eslint-plugin-sonarjs": ">= 3",
    "eslint-plugin-unicorn": ">= 62"
  },
  "packageManager": "yarn@4.11.0",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
